  dns-nameservers <TMPL_VAR netzwerknameserver_IPv6>
 </TMPL_UNLESS dhcp>
<TMPL_IF dhcp>
  request_prefix 1
</TMPL_IF>
  autoconf 1
  accept_ra 2
  